program begin

  integer suma(integer a, integer b, integer c, integer d, integer e, integer f, integer g, integer h, integer i, integer j) begin
    return (a+b+c+d+e+f+g+h+i+j);
  end

  void main() begin
    integer a,b,c,d,e,f,g,h,i,j,k;
    a=0;b=1;c=2;d=3;e=4;f=5;g=6;h=7;i=8;j=9;
    k = suma(a,b,c,d,e,f,g,h,i,j);
    if (k == 1) then begin
      suma(a,b,c,d,e,f,g,h,i,j);
    end 
    else begin
      suma(j,i,h,g,f,e,d,c,b,a);
    end
  end
end